{
    "compilerOptions": {
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true,
        "target": "ES5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "./dist",
        "declaration": false
    },
    "typeRoots": [
        "./typings"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}
